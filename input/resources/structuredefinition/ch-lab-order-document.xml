<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="ch-lab-order-document"/>
  <url value="http://fhir.ch/ig/ch-lab-order/StructureDefinition/ch-lab-order-document"/>
  <version value="0.1.0"/>
  <name value="CHLAB_ORDERDocument"/>
  <title value="CH LAB-ORDER Document Profile"/>
  <status value="active"/>
  <experimental value="false"/>
  <publisher value="eHealth Suisse"/>
  <description value="Definition of the bundle for the lab order document"/>
  <kind value="resource"/>
  <abstract value="false"/>
  <type value="Bundle"/>
  <baseDefinition value="http://fhir.ch/ig/ch-core/StructureDefinition/ch-core-document-epr"/>
  <derivation value="constraint"/>
  
  <differential>
  
     <element id="Bundle">
        <path value="Bundle"/>
        <short value="CH LAB-ORDER Document"/>
     </element>
     <element id="Bundle.identifier">
        <path value="Bundle.identifier"/>
        <short value="Identification number of the document"/>
        <min value="1"/>
        <max value="1"/>
     </element>
     <element id="Bundle.identifier.system">
        <path value="Bundle.identifier.system"/>
        <min value="1"/>
        <max value="1"/>
     </element>
     <element id="Bundle.identifier.value">
        <path value="Bundle.identifier.value"/>
        <min value="1"/>
     </element>
     <element id="Bundle.type">
        <path value="Bundle.type" />
        <fixedCode value="document" />
     </element>
     <element id="Bundle.timestamp">
        <path value="Bundle.timestamp" />
        <short value="The document's creation date and time"/>
     </element>
     <element id="Bundle.entry">
        <path value="Bundle.entry" />
        <slicing>
           <discriminator>
              <type value="profile" />
              <path value="resource" />
           </discriminator>
           <rules value="open" />
        </slicing>
     </element>
     <element id="Bundle.entry:Composition">
        <path value="Bundle.entry" />
        <sliceName value="Composition" />
        <short value="Lab order composition" />
        <min value="1" />
        <max value="1" />
     </element>
     <element id="Bundle.entry:Composition.resource">
        <path value="Bundle.entry.resource" />
        <min value="1" />
        <type>
           <code value="Resource" />
           <profile value="http://fhir.ch/ig/ch-lab-order/StructureDefinition/ch-lab-order-composition" />
        </type>
     </element>

  </differential>
</StructureDefinition>